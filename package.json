{
  "name": "feastfind-api",
  "private": "true",
  "type": "module",
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun run --hot src/index.ts",
    "seed": "bun --env-file .env prisma db seed",
    "generate": "bun --env-file .env prisma generate",
    "migrate": "bun --env-file .env prisma migrate deploy",
    "dev:test": "bun --env-file .env.test run --hot src/index.ts",
    "seed:test": "bun --env-file .env.test prisma db seed",
    "generate:test": "bun --env-file .env.test prisma generate",
    "migrate:test": "bun --env-file .env.test prisma migrate deploy"
  },
  "prisma": {
    "seed": "bun prisma/seed.ts"
  },
  "dependencies": {
    "@hono/zod-openapi": "^0.18.3",
    "@prisma/client": "^6.0.1",
    "@scalar/hono-api-reference": "^0.5.164",
    "hono": "^4.6.13",
    "zod": "^3.24.0",
    "zod-prisma-types": "^3.2.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "prettier-plugin-prisma": "^5.0.0",
    "prisma": "^6.0.1"
  }
}