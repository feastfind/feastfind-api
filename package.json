    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force --skip-seed"
  },
  "prisma": {
    "seed": "bun prisma/seed.ts"
  },
  "dependencies": {
    "@hono/zod-openapi": "^0.18.3",
    "@prisma/client": "^6.0.1",
    "@scalar/hono-api-reference": "^0.5.164",
    "hono": "^4.6.13",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "prettier-plugin-prisma": "^5.0.0",
    "prisma": "^6.0.1",
    "zod-prisma-types": "^3.2.1"
  }
}
